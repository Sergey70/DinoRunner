platform :ios, '12.0'

source 'https://github.com/CocoaPods/Specs.git'

workspace 'DinoRunner.xcworkspace'

# === Swift Demo ===

target 'DinoRunner-Demo' do
  project 'DinoRunner-Demo/DinoRunner-Demo.xcodeproj'
  use_frameworks!
  
  pod 'DinoRunner'

end

# === Objective-C Demo ===

target 'DinoRunnerObjC-Demo' do
  project 'DinoRunnerObjC-Demo/DinoRunnerObjc-Demo.xcodeproj'
  use_frameworks!
  pod 'DinoRunner'

end
